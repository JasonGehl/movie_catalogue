{
  "name": "movie-catalogue",
  "version": "0.0.1",
  "description": "A personal movie collection catalogue",
  "scripts": {
    "prestart": "babel-node build/startMessage.js",
    "start": "npm-run-all --parallel security-check start-server lint:watch test:watch ",
    "lint": "esw webpack.config.* src build_scripts --color",
    "lint:watch": "npm run lint -- --watch",
    "start-server": "babel-node build/devServer.js",
    "security-check": "nsp check",
    "test": "nyc mocha build/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Jason Gehl",
  "homepage": "https://github.com/JasonGehl/movie_catalogue#readme",
  "license": "MIT",
  "repository": "github:JasonGehl/movie_catalogue",
  "dependencies": {
    "angular": "^1.6.10"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-env": "^1.7.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "compression": "^1.7.2",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.14.0",
    "mocha": "^5.2.0",
    "nock": "^9.3.2",
    "npm-run-all": "3.1.1",
    "nsp": "^3.2.1",
    "nyc": "^12.0.2",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
